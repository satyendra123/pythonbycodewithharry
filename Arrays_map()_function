/* EX-1
const x = [1, 2, 3, 4, 5];
const y = x.map(item => {
  console.log(item);
  return item; // Map creates a new array, so you need to return the item
});

console.log(y)
*/

/* EX-2
const x = [1, 2, 3, 4, 5];

function extract(data) {
  return data.map(item => {
    console.log(item);
    return item;
  });
}

const y = extract(x);
console.log(y);
*/

