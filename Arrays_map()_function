//EX-4
const x = [1, 2, 3, 4, 5];
const y = x.map(item => item);
console.log(y);

//EX-5
const x = [1, 2, 3, 4, 5];
const y = x.map((item, index) => {
  console.log(`Key: ${index}, Value: ${item}`);
  return item;
});

extract(x); // Call the extract function to log each item in array x
